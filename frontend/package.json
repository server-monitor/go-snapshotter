  "scripts": {
    "lint": "eslint --config /main/system/javascript/DOT.eslintrc.yml",
    "gensymlinks": "mkdir www 2>/dev/null; cd www && for file in index.html debug.js; do ln -sf \"../src/$file\"; done",
    "prestart": "npm run gensymlinks",
    "start": "webpack-dev-server --progress --watch --display-error-details --display-cached",
    "infer-imports": "node ./spec/helpers/infer_imports.js",
    "test": "node_modules/.bin/karma start karma.config.js",
    "test:dev": "npm run test -- --watch",
    "ptest:no-watch": "mocha --growl --require babel-register --require ignore-styles spec/helpers/utilities.js",
    "ptest": "npm run test:no-watch -- --watch",
    "collect-test-files": "find spec/ -type f -name '*.spec.js'",
    "test:all:no-watch": "npm run --silent collect-test-files | xargs npm run test:no-watch --recursive",
    "test:all": "npm run --silent collect-test-files | xargs npm test --recursive",
    "precompile": "mkdir www 2>/dev/null; cd www && for file in index.html debug.js; do /bin/cp --remove-destination \"../src/$file\" .; done",
    "compile": "webpack",
    "postcompile": "notify-send --expire-time=10000 'npm compile done'"
